{
  "name": "flow-store",
  "version": "1.3.0",
  "main": "dist/src/main.prod.js",
  "scripts": {
    "build:next": "next build next",
    "copy:standalone": "mkdir -p next/.next/standalone/.next && cp -R next/public next/.next/standalone/public && cp -R next/.next/static next/.next/standalone/.next/static",
    "build:electron": "node -e \"try{require('fs').rmSync('.next/types',{recursive:true,force:true});}catch(e){}\" && tsc && mkdir -p dist && cp -R assets dist/",
    "build": "npm run build:next && npm run copy:standalone && npm run build:electron",
    "start": "npm run build && electron dist/src/main.prod.js",
    "start:prod": "NODE_ENV=production npm run build && NODE_ENV=production electron dist/src/main.prod.js",
    "dev:prod": "NODE_ENV=production npm run dev:next",
    "dev": "npm run build:electron && electron dist/src/main.dev.js",
    "dev:next": "next dev next",
    "pack": "npm run build && electron-forge package",
    "pack:mac": "npm run build && electron-forge package --platform=darwin",
    "pack:win": "npm run build && electron-forge package --platform=win32 --arch=x64",
    "pack:win:arm64": "npm run build && electron-forge package --platform=win32 --arch=arm64",
    "make": "npm run build && electron-forge make",
    "make:mac": "npm run build && electron-forge make --platform=darwin",
    "make:win": "npm run build && electron-forge make --platform=win32 --arch=x64",
    "seed": "node run-seed.js test",
    "seed:test": "node run-seed.js test",
    "seed:prod": "node run-seed.js production",
    "seed:units": "ts-node data/seed/seed-units.ts",
    "seed:flowstore": "ts-node data/seed/seed-flowstore.ts",
    "seed:all": "npm run seed:test && npm run seed:prod",
    "sync-db": "ts-node scripts/sync-databases.ts",
    "migrate:production": "ts-node data/migrations/migrate-to-test-schema.ts",
    "verify:migration": "ts-node data/migrations/verify-migration.ts",
    "test:setup": "ts-node tests/scripts/setup-test-db.ts",
    "test:seed": "ts-node tests/scripts/seed-test-data.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:report": "playwright show-report"
  },
  "keywords": [],
  "author": "Felipe",
  "license": "ISC",
  "description": "Electron Next.js Starter",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.57.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/electron": "^1.4.38",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "electron": "^39.2.3",
    "electron-icon-builder": "^2.0.1",
    "electron-icon-maker": "^0.0.5",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.7",
    "fs-extra": "^11.3.2",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fontsource/material-symbols-outlined": "^5.2.30",
    "@fontsource/material-symbols-rounded": "^5.2.30",
    "@fontsource/material-symbols-sharp": "^5.2.30",
    "@types/leaflet": "^1.9.21",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "electron-squirrel-startup": "^1.0.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.561.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mysql2": "^3.6.5",
    "next": "^15.3.3",
    "next-auth": "^4.24.13",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.6.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27",
    "xlsx": "^0.18.5"
  }
}
